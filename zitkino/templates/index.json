{
    "data": [
        {% for showtime in showtimes %}
            {
                "title": {{ showtime.film.title_main|tojson|safe }},
                "hash": {{ showtime.id|string|tojson|safe }},
                "cinema": {{ showtime.cinema.name|tojson|safe }},
                "date": {{ showtime.starts_at_day|string|tojson|safe }}
            }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}
