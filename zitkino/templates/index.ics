BEGIN:VCALENDAR
PRODID:-//Honza Javorek//Žít kino//CS
VERSION:2.0
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME:Kino
X-WR-TIMEZONE:Europe/Prague
X-WR-CALDESC;CHARSET=UTF-8:Nejlepší program kin v Brně.
{% for showtime in showtimes %}
BEGIN:VEVENT
DTSTART;VALUE=DATE:{{ showtime.starts_at_day|format_date_ics }}
DTEND;VALUE=DATE:{{ showtime.starts_at_day|format_date_ics }}
DTSTAMP:{{ now|format_timestamp_ics }}
UID:{{ showtime.id }}@honzajavorek.cz
CREATED:{{ now|format_timestamp_ics }}
DESCRIPTION:http://zitkino.cz#id{{ showtime.id }}
LAST-MODIFIED:{{ now|format_timestamp_ics }}
LOCATION:kino {{ showtime.cinema.name }}, Brno
SEQUENCE:0
STATUS:CONFIRMED
SUMMARY;CHARSET=UTF-8:{{ showtime.film.title_main }} ({{ showtime.cinema.name }})
TRANSP:TRANSPARENT
END:VEVENT
{% endfor %}
END:VCALENDAR
